{
	"browser": true,		// defines globals exposed by modern browsers
	"node": true,			// node.js vars
	"boss": true,			// suppresses warnings about the use of assignments in cases where comparisons are expected
	"curly": true,			// requires you to always put curly braces around blocks in loops and conditionals
	"camelcase": true,		// force all variable names to use camelCase
	"esnext": true,			// ECMAScript 6 specific syntax
	"eqeqeq": true,			// This options prohibits the use of == and != in favor of === and !==
	//"eqnull": true,			// suppresses warnings about == null comparisons
	"expr": true,			// suppresses warnings about the use of expressions where normally you would expect to see assignments or function calls
	//"immed": true,			// prohibits the use of immediate function invocations without wrapping them in parentheses
	"noarg": true,			// prohibits the use of arguments.caller and arguments.callee
	"quotmark": "double",	// enforces the consistency of quotation marks used throughout your code
	"undef": true,			// prohibits the use of explicitly undeclared variables
	//"unused": true,			// warns when you define and never use your variables
	"newcap": true,			// requires you to capitalize names of constructor functions
	"jquery": true,			// jQuery global vars
	"globals" : {
		"chrome": true		// Chrome global vars
	}
}
